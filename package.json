		"lint:wc": "polymer lint -i \"./*\" \"components\" \"demo\" \"test\"",
		"test:polymer:local": "polymer test --skip-plugin sauce",
		"test:polymer:sauce": "polymer test --skip-plugin local",
		"test": "npm run lint && npm run test:polymer:local"
	},
