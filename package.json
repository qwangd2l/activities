	},
	"name": "d2l-activities",
	"scripts": {
		"build": "gulp build",
		"install-attest": "npm install attest --registry=https://d2lartifacts.artifactoryonline.com/d2lartifacts/api/npm/attest/ && node scripts/remove_attest_from_project.js",
		"lint": "npm run lint:wc && npm run lint:js",
